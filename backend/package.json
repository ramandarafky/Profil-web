{
  "name": "portfolio-backend",
  "version": "1.0.0",
  "description": "Scalable backend API for portfolio application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:build": "docker build -t portfolio-backend .",
    "docker:run": "docker run -p 3001:3001 portfolio-backend",
    "migrate": "node database/migrate.js",
    "seed": "node database/seed.js"
  },
  "keywords": [
    "portfolio",
    "api",
    "scalable",
    "nodejs",
    "express"
  ],
  "author": "Alex Chen",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "redis": "^4.6.11",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
